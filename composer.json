{
    "name": "albegali/acl-bundle",
    "type": "symfony-bundle",
    "description": "The missing link between Symfony`s Acl implementation and your application.",
    "keywords": ["symfony2", "acl", "security"],
    "homepage": "http://albegali.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Schmid",
            "email": "js@1up.io",
            "homepage": "http://1up.io",
            "role": "Developer"
        },
        {
            "name": "David Greminger",
            "email": "dg@1up.io",
            "homepage": "http://1up.io",
            "role": "Developer"
        }
    ],

    "require": {
        "symfony/framework-bundle": "3.0.*",
        "symfony/security-bundle": "3.0.*",
        "symfony/expression-language": "3.0.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/common": "~2.5",
        "doctrine/dbal": "~2.5"
    },

    "require-dev": {
        "symfony/finder": "3.0.*",
        "symfony/browser-kit": "3.0.*",
        "sensio/framework-extra-bundle": "3.0.*",
        "doctrine/orm": "~2.4",
        "phpunit/phpunit": "3.7.*"
    },

    "suggest": {
        "sensio/framework-extra-bundle": "Allows you to use controller annotations.",
        "doctrine/orm": "Allows you to remove Acl entries automatically."
    },

    "autoload": {
        "psr-0": { "Albegali\\AclBundle": "" }
    },

    "target-dir": "Albegali/AclBundle"
}
